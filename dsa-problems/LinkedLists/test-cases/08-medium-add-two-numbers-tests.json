{
  "problem": "Add Two Numbers",
  "function_name": "add_two_numbers",
  "test_cases": [
    {
      "id": 1,
      "description": "342 + 465 = 807",
      "input": {"l1": [2, 4, 3], "l2": [5, 6, 4]},
      "expected_output": [7, 0, 8],
      "category": "base_case"
    },
    {
      "id": 2,
      "description": "0 + 0 = 0",
      "input": {"l1": [0], "l2": [0]},
      "expected_output": [0],
      "category": "corner_case"
    },
    {
      "id": 3,
      "description": "999 + 9999 = 10998",
      "input": {"l1": [9, 9, 9], "l2": [9, 9, 9, 9]},
      "expected_output": [8, 9, 9, 0, 1],
      "category": "base_case"
    },
    {
      "id": 4,
      "description": "5 + 5 = 10",
      "input": {"l1": [5], "l2": [5]},
      "expected_output": [0, 1],
      "category": "base_case"
    },
    {
      "id": 5,
      "description": "321 + 321 = 642",
      "input": {"l1": [1, 2, 3], "l2": [1, 2, 3]},
      "expected_output": [2, 4, 6],
      "category": "base_case"
    },
    {
      "id": 6,
      "description": "1 + 999 = 1000",
      "input": {"l1": [1], "l2": [9, 9, 9]},
      "expected_output": [0, 0, 0, 1],
      "category": "edge_case"
    },
    {
      "id": 7,
      "description": "9999 + 1 = 10000",
      "input": {"l1": [9, 9, 9, 9], "l2": [1]},
      "expected_output": [0, 0, 0, 0, 1],
      "category": "edge_case"
    },
    {
      "id": 8,
      "description": "0 + 321 = 321",
      "input": {"l1": [0], "l2": [1, 2, 3]},
      "expected_output": [1, 2, 3],
      "category": "base_case"
    },
    {
      "id": 9,
      "description": "7654321 + 1234567 = 8888888",
      "input": {"l1": [1, 2, 3, 4, 5, 6, 7], "l2": [7, 6, 5, 4, 3, 2, 1]},
      "expected_output": [8, 8, 8, 8, 8, 8, 8],
      "category": "base_case"
    },
    {
      "id": 10,
      "description": "99 + 1 = 100",
      "input": {"l1": [9, 9], "l2": [1]},
      "expected_output": [0, 0, 1],
      "category": "boundary"
    },
    {
      "id": 11,
      "description": "99999 + 1 = 100000",
      "input": {"l1": [9, 9, 9, 9, 9], "l2": [1]},
      "expected_output": [0, 0, 0, 0, 0, 1],
      "category": "edge_case"
    },
    {
      "id": 12,
      "description": "852 + 147 = 999",
      "input": {"l1": [2, 5, 8], "l2": [7, 4, 1]},
      "expected_output": [9, 9, 9],
      "category": "base_case"
    },
    {
      "id": 13,
      "description": "12345 + 67890 = 80235",
      "input": {"l1": [5, 4, 3, 2, 1], "l2": [0, 9, 8, 7, 6]},
      "expected_output": [5, 3, 2, 0, 8],
      "category": "stress_test"
    },
    {
      "id": 14,
      "description": "1000 + 1000 = 2000",
      "input": {"l1": [0, 0, 0, 1], "l2": [0, 0, 0, 1]},
      "expected_output": [0, 0, 0, 2],
      "category": "base_case"
    },
    {
      "id": 15,
      "description": "10101 + 99999 = 110100",
      "input": {"l1": [1, 0, 1, 0, 1], "l2": [9, 9, 9, 9, 9]},
      "expected_output": [0, 0, 1, 0, 1, 1],
      "category": "large_case"
    }
  ]
}
