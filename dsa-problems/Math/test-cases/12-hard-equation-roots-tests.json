{
  "problem": "Quadratic Equation Solver",
  "function_name": "solve_quadratic",
  "test_cases": [
    {
      "id": 1,
      "description": "Two distinct positive roots",
      "input": {"a": 1, "b": -5, "c": 6},
      "expected_output": [2.00, 3.00],
      "category": "basic"
    },
    {
      "id": 2,
      "description": "One repeated root",
      "input": {"a": 1, "b": -4, "c": 4},
      "expected_output": [2.00, 2.00],
      "category": "edge_case"
    },
    {
      "id": 3,
      "description": "No real roots (complex)",
      "input": {"a": 1, "b": 0, "c": 4},
      "expected_output": [],
      "category": "edge_case"
    },
    {
      "id": 4,
      "description": "Two positive roots different",
      "input": {"a": 2, "b": -8, "c": 6},
      "expected_output": [1.00, 3.00],
      "category": "basic"
    },
    {
      "id": 5,
      "description": "Negative repeated root",
      "input": {"a": 1, "b": 2, "c": 1},
      "expected_output": [-1.00, -1.00],
      "category": "edge_case"
    },
    {
      "id": 6,
      "description": "Roots of opposite signs",
      "input": {"a": 1, "b": -2, "c": -3},
      "expected_output": [-1.00, 3.00],
      "category": "basic"
    },
    {
      "id": 7,
      "description": "Zero is one root",
      "input": {"a": 1, "b": 5, "c": 0},
      "expected_output": [-5.00, 0.00],
      "category": "edge_case"
    },
    {
      "id": 8,
      "description": "Large coefficients",
      "input": {"a": 1, "b": -1000, "c": 999},
      "expected_output": [1.00, 999.00],
      "category": "boundary"
    },
    {
      "id": 9,
      "description": "Negative a coefficient",
      "input": {"a": -1, "b": 5, "c": -6},
      "expected_output": [2.00, 3.00],
      "category": "basic"
    },
    {
      "id": 10,
      "description": "Both negative roots",
      "input": {"a": 1, "b": 5, "c": 6},
      "expected_output": [-3.00, -2.00],
      "category": "basic"
    },
    {
      "id": 11,
      "description": "Fractional roots",
      "input": {"a": 2, "b": -5, "c": 2},
      "expected_output": [0.50, 2.00],
      "category": "basic"
    },
    {
      "id": 12,
      "description": "Sum and product relation",
      "input": {"a": 1, "b": -7, "c": 12},
      "expected_output": [3.00, 4.00],
      "category": "basic"
    },
    {
      "id": 13,
      "description": "Perfect square trinomial positive",
      "input": {"a": 1, "b": 6, "c": 9},
      "expected_output": [-3.00, -3.00],
      "category": "edge_case"
    },
    {
      "id": 14,
      "description": "Discriminant nearly zero",
      "input": {"a": 1, "b": 2, "c": 0.99},
      "expected_output": [-1.10, -0.90],
      "category": "boundary"
    },
    {
      "id": 15,
      "description": "Simple factorizable",
      "input": {"a": 1, "b": 0, "c": -9},
      "expected_output": [-3.00, 3.00],
      "category": "basic"
    }
  ]
}
