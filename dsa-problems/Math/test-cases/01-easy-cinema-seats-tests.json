{
  "problem": "Cinema Seat Arrangement",
  "function_name": "check_cinema_capacity",
  "test_cases": [
    {
      "id": 1,
      "description": "Example - people can be seated with empty seats remaining",
      "input": {"rows": 10, "seatsPerRow": 15, "totalPeople": 120},
      "expected_output": [1, 30],
      "category": "basic"
    },
    {
      "id": 2,
      "description": "Example - cannot seat all people",
      "input": {"rows": 5, "seatsPerRow": 8, "totalPeople": 50},
      "expected_output": [0, -1],
      "category": "basic"
    },
    {
      "id": 3,
      "description": "Exact fit - no empty seats",
      "input": {"rows": 20, "seatsPerRow": 25, "totalPeople": 500},
      "expected_output": [1, 0],
      "category": "edge_case"
    },
    {
      "id": 4,
      "description": "Minimum values - single seat single person",
      "input": {"rows": 1, "seatsPerRow": 1, "totalPeople": 1},
      "expected_output": [1, 0],
      "category": "corner_case"
    },
    {
      "id": 5,
      "description": "One person in large cinema",
      "input": {"rows": 100, "seatsPerRow": 50, "totalPeople": 1},
      "expected_output": [1, 4999],
      "category": "edge_case"
    },
    {
      "id": 6,
      "description": "Maximum rows and seats",
      "input": {"rows": 500, "seatsPerRow": 100, "totalPeople": 50000},
      "expected_output": [1, 0],
      "category": "boundary"
    },
    {
      "id": 7,
      "description": "One more person than capacity",
      "input": {"rows": 10, "seatsPerRow": 10, "totalPeople": 101},
      "expected_output": [0, -1],
      "category": "edge_case"
    },
    {
      "id": 8,
      "description": "One less person than capacity",
      "input": {"rows": 10, "seatsPerRow": 10, "totalPeople": 99},
      "expected_output": [1, 1],
      "category": "edge_case"
    },
    {
      "id": 9,
      "description": "Single row cinema",
      "input": {"rows": 1, "seatsPerRow": 50, "totalPeople": 30},
      "expected_output": [1, 20],
      "category": "basic"
    },
    {
      "id": 10,
      "description": "Single seat per row",
      "input": {"rows": 100, "seatsPerRow": 1, "totalPeople": 75},
      "expected_output": [1, 25],
      "category": "basic"
    },
    {
      "id": 11,
      "description": "Large capacity small group",
      "input": {"rows": 500, "seatsPerRow": 100, "totalPeople": 100},
      "expected_output": [1, 49900],
      "category": "stress_test"
    },
    {
      "id": 12,
      "description": "Maximum people constraint",
      "input": {"rows": 500, "seatsPerRow": 100, "totalPeople": 100000},
      "expected_output": [0, -1],
      "category": "boundary"
    },
    {
      "id": 13,
      "description": "Half capacity booking",
      "input": {"rows": 20, "seatsPerRow": 20, "totalPeople": 200},
      "expected_output": [1, 200],
      "category": "basic"
    },
    {
      "id": 14,
      "description": "Zero people edge case",
      "input": {"rows": 10, "seatsPerRow": 10, "totalPeople": 1},
      "expected_output": [1, 99],
      "category": "corner_case"
    },
    {
      "id": 15,
      "description": "Prime number capacity",
      "input": {"rows": 7, "seatsPerRow": 11, "totalPeople": 77},
      "expected_output": [1, 0],
      "category": "basic"
    }
  ]
}
