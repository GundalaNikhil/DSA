{
  "problem": "Multi-Tier Discount Calculator",
  "function_name": "calculate_final_price",
  "test_cases": [
    {
      "id": 1,
      "description": "10% tier no loyalty",
      "input": {"cartValue": 150, "hasLoyalty": false},
      "expected_output": 135.00,
      "category": "basic"
    },
    {
      "id": 2,
      "description": "10% tier with loyalty",
      "input": {"cartValue": 150, "hasLoyalty": true},
      "expected_output": 128.25,
      "category": "basic"
    },
    {
      "id": 3,
      "description": "20% tier with loyalty",
      "input": {"cartValue": 1200, "hasLoyalty": true},
      "expected_output": 912.00,
      "category": "basic"
    },
    {
      "id": 4,
      "description": "No discount with loyalty",
      "input": {"cartValue": 75, "hasLoyalty": true},
      "expected_output": 71.25,
      "category": "edge_case"
    },
    {
      "id": 5,
      "description": "15% tier no loyalty",
      "input": {"cartValue": 500, "hasLoyalty": false},
      "expected_output": 425.00,
      "category": "basic"
    },
    {
      "id": 6,
      "description": "Minimum cart value",
      "input": {"cartValue": 1, "hasLoyalty": false},
      "expected_output": 1.00,
      "category": "corner_case"
    },
    {
      "id": 7,
      "description": "Exactly 100 boundary",
      "input": {"cartValue": 100, "hasLoyalty": false},
      "expected_output": 90.00,
      "category": "boundary"
    },
    {
      "id": 8,
      "description": "Just under 100",
      "input": {"cartValue": 99, "hasLoyalty": false},
      "expected_output": 99.00,
      "category": "boundary"
    },
    {
      "id": 9,
      "description": "Exactly 500 boundary",
      "input": {"cartValue": 500, "hasLoyalty": true},
      "expected_output": 403.75,
      "category": "boundary"
    },
    {
      "id": 10,
      "description": "Exactly 1000 boundary",
      "input": {"cartValue": 1000, "hasLoyalty": false},
      "expected_output": 800.00,
      "category": "boundary"
    },
    {
      "id": 11,
      "description": "Maximum cart value",
      "input": {"cartValue": 10000, "hasLoyalty": true},
      "expected_output": 7600.00,
      "category": "boundary"
    },
    {
      "id": 12,
      "description": "15% tier with loyalty",
      "input": {"cartValue": 750, "hasLoyalty": true},
      "expected_output": 605.63,
      "category": "basic"
    },
    {
      "id": 13,
      "description": "Mid-range 10% tier",
      "input": {"cartValue": 250, "hasLoyalty": false},
      "expected_output": 225.00,
      "category": "basic"
    },
    {
      "id": 14,
      "description": "Just under 500",
      "input": {"cartValue": 499, "hasLoyalty": false},
      "expected_output": 449.10,
      "category": "boundary"
    },
    {
      "id": 15,
      "description": "Just under 1000",
      "input": {"cartValue": 999, "hasLoyalty": true},
      "expected_output": 806.69,
      "category": "boundary"
    }
  ]
}
