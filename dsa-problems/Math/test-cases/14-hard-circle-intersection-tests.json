{
  "problem": "Circle Intersection Area",
  "function_name": "circle_intersection_area",
  "test_cases": [
    {
      "id": 1,
      "description": "Partial overlap",
      "input": {"x1": 0, "y1": 0, "r1": 2, "x2": 3, "y2": 0, "r2": 2},
      "expected_output": 3.48,
      "category": "basic"
    },
    {
      "id": 2,
      "description": "Identical circles",
      "input": {"x1": 0, "y1": 0, "r1": 1, "x2": 0, "y2": 0, "r2": 1},
      "expected_output": 3.14,
      "category": "edge_case"
    },
    {
      "id": 3,
      "description": "No intersection - too far",
      "input": {"x1": 0, "y1": 0, "r1": 1, "x2": 5, "y2": 0, "r2": 1},
      "expected_output": 0.00,
      "category": "edge_case"
    },
    {
      "id": 4,
      "description": "One inside another",
      "input": {"x1": 0, "y1": 0, "r1": 5, "x2": 0, "y2": 0, "r2": 2},
      "expected_output": 12.57,
      "category": "basic"
    },
    {
      "id": 5,
      "description": "Touching externally",
      "input": {"x1": 0, "y1": 0, "r1": 3, "x2": 6, "y2": 0, "r2": 3},
      "expected_output": 0.00,
      "category": "edge_case"
    },
    {
      "id": 6,
      "description": "Larger overlap",
      "input": {"x1": 0, "y1": 0, "r1": 3, "x2": 4, "y2": 0, "r2": 3},
      "expected_output": 7.25,
      "category": "basic"
    },
    {
      "id": 7,
      "description": "Circles on y-axis",
      "input": {"x1": 0, "y1": 0, "r1": 2, "x2": 0, "y2": 2, "r2": 2},
      "expected_output": 5.62,
      "category": "basic"
    },
    {
      "id": 8,
      "description": "Large radii",
      "input": {"x1": 0, "y1": 0, "r1": 1000, "x2": 500, "y2": 0, "r2": 1000},
      "expected_output": 2393286.55,
      "category": "boundary"
    },
    {
      "id": 9,
      "description": "Small overlap",
      "input": {"x1": 0, "y1": 0, "r1": 5, "x2": 9, "y2": 0, "r2": 5},
      "expected_output": 4.14,
      "category": "basic"
    },
    {
      "id": 10,
      "description": "Unit circles half overlap",
      "input": {"x1": 0, "y1": 0, "r1": 1, "x2": 1, "y2": 0, "r2": 1},
      "expected_output": 1.23,
      "category": "basic"
    },
    {
      "id": 11,
      "description": "Diagonal positioning",
      "input": {"x1": 0, "y1": 0, "r1": 5, "x2": 3, "y2": 4, "r2": 5},
      "expected_output": 28.59,
      "category": "basic"
    },
    {
      "id": 12,
      "description": "Small circle inside large",
      "input": {"x1": 0, "y1": 0, "r1": 10, "x2": 2, "y2": 2, "r2": 3},
      "expected_output": 28.27,
      "category": "basic"
    },
    {
      "id": 13,
      "description": "Almost concentric",
      "input": {"x1": 0, "y1": 0, "r1": 5, "x2": 0.1, "y2": 0.1, "r2": 5},
      "expected_output": 78.26,
      "category": "edge_case"
    },
    {
      "id": 14,
      "description": "Negative coordinates",
      "input": {"x1": -5, "y1": -5, "r1": 3, "x2": -3, "y2": -3, "r2": 3},
      "expected_output": 13.05,
      "category": "basic"
    },
    {
      "id": 15,
      "description": "Different radii overlap",
      "input": {"x1": 0, "y1": 0, "r1": 4, "x2": 5, "y2": 0, "r2": 2},
      "expected_output": 4.40,
      "category": "basic"
    }
  ]
}
