      "input": {
        "grid": [[10, -5], [-3, 8]]
      },
      "expected_output": 15,
      "category": "basic"
    },
    {
      "id": 3,
      "description": "Single cell",
      "input": {
        "grid": [[5]]
      },
      "expected_output": 5,
      "category": "corner_case"
    },
    {
      "id": 4,
      "description": "Example - all positive",
      "input": {
        "grid": [[1, 2, 3], [4, 5, 6]]
      },
      "expected_output": 16,
      "category": "basic"
    },
    {
      "id": 5,
