{
  "problem": "Minimum Window Substring",
  "function_name": "min_window_substring",
  "test_cases": [
    {
      "id": 1,
      "description": "BANC contains A,B,C",
      "input": { "document": "ADOBECODEBANC", "query": "ABC" },
      "expected_output": "BANC",
      "category": "base_case"
    },
    {
      "id": 2,
      "description": "Exact match",
      "input": { "document": "a", "query": "a" },
      "expected_output": "a",
      "category": "corner_case"
    },
    {
      "id": 3,
      "description": "Not enough characters",
      "input": { "document": "a", "query": "aa" },
      "expected_output": "",
      "category": "edge_case"
    },
    {
      "id": 4,
      "description": "Entire string needed",
      "input": { "document": "abc", "query": "cba" },
      "expected_output": "abc",
      "category": "base_case"
    },
    {
      "id": 5,
      "description": "Query at beginning",
      "input": { "document": "abcdef", "query": "ab" },
      "expected_output": "ab",
      "category": "base_case"
    },
    {
      "id": 6,
      "description": "Query at end",
      "input": { "document": "abcdef", "query": "ef" },
      "expected_output": "ef",
      "category": "base_case"
    },
    {
      "id": 7,
      "description": "Duplicate characters in query",
      "input": { "document": "aaab", "query": "aa" },
      "expected_output": "aa",
      "category": "base_case"
    },
    {
      "id": 8,
      "description": "No valid window",
      "input": { "document": "xyz", "query": "abc" },
      "expected_output": "",
      "category": "edge_case"
    },
    {
      "id": 9,
      "description": "Multiple valid windows",
      "input": { "document": "aabdec", "query": "abc" },
      "expected_output": "abdec",
      "category": "base_case"
    },
    {
      "id": 10,
      "description": "Single char multiple times",
      "input": { "document": "aaaaaa", "query": "a" },
      "expected_output": "a",
      "category": "base_case"
    },
    {
      "id": 11,
      "description": "Query longer than document",
      "input": { "document": "ab", "query": "abc" },
      "expected_output": "",
      "category": "edge_case"
    },
    {
      "id": 12,
      "description": "Complex with repeats",
      "input": { "document": "ABBCAB", "query": "AB" },
      "expected_output": "AB",
      "category": "stress_test"
    },
    {
      "id": 13,
      "description": "Minimum at start",
      "input": { "document": "abczzzzzzz", "query": "abc" },
      "expected_output": "abc",
      "category": "base_case"
    },
    {
      "id": 14,
      "description": "Case with all same",
      "input": { "document": "bbbba", "query": "ab" },
      "expected_output": "ba",
      "category": "base_case"
    },
    {
      "id": 15,
      "description": "Long document short query",
      "input": { "document": "abcdefghijklmnop", "query": "nop" },
      "expected_output": "nop",
      "category": "large_case"
    }
  ]
}
