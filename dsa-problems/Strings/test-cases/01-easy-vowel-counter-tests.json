{
  "problem": "Vowel Counter",
  "function_name": "count_vowels",
  "test_cases": [
    {
      "id": 1,
      "description": "Example case with mixed case",
      "input": {"text": "Hello World"},
      "expected_output": 3,
      "category": "base_case"
    },
    {
      "id": 2,
      "description": "Only vowels lowercase",
      "input": {"text": "aeiou"},
      "expected_output": 5,
      "category": "base_case"
    },
    {
      "id": 3,
      "description": "No vowels",
      "input": {"text": "xyz"},
      "expected_output": 0,
      "category": "edge_case"
    },
    {
      "id": 4,
      "description": "Single vowel",
      "input": {"text": "a"},
      "expected_output": 1,
      "category": "corner_case"
    },
    {
      "id": 5,
      "description": "Single consonant",
      "input": {"text": "b"},
      "expected_output": 0,
      "category": "corner_case"
    },
    {
      "id": 6,
      "description": "String with spaces",
      "input": {"text": "The quick brown fox"},
      "expected_output": 5,
      "category": "base_case"
    },
    {
      "id": 7,
      "description": "Programming example",
      "input": {"text": "Programming"},
      "expected_output": 3,
      "category": "base_case"
    },
    {
      "id": 8,
      "description": "All uppercase vowels",
      "input": {"text": "AEIOU"},
      "expected_output": 5,
      "category": "base_case"
    },
    {
      "id": 9,
      "description": "All lowercase consonants",
      "input": {"text": "bcdfghjklmnpqrstvwxyz"},
      "expected_output": 0,
      "category": "edge_case"
    },
    {
      "id": 10,
      "description": "Repeating vowels",
      "input": {"text": "aaaaaeeeeeiiiiiooooouuuuu"},
      "expected_output": 25,
      "category": "base_case"
    },
    {
      "id": 11,
      "description": "Long sentence",
      "input": {"text": "Programming is awesome and exciting"},
      "expected_output": 13,
      "category": "base_case"
    },
    {
      "id": 12,
      "description": "Mixed upper and lower vowels",
      "input": {"text": "AeIoU aEiOu"},
      "expected_output": 10,
      "category": "base_case"
    },
    {
      "id": 13,
      "description": "Empty feeling word",
      "input": {"text": "rhythm"},
      "expected_output": 0,
      "category": "edge_case"
    },
    {
      "id": 14,
      "description": "Multiple spaces with vowels",
      "input": {"text": "a e i o u"},
      "expected_output": 5,
      "category": "base_case"
    },
    {
      "id": 15,
      "description": "Long string with many vowels",
      "input": {"text": "The quick brown fox jumps over the lazy dog and eats some nice apples"},
      "expected_output": 22,
      "category": "stress_test"
    }
  ]
}
